#include <SoftwareSerial.h>
SoftwareSerial mySerial(11, 12);  //Rx=11, Tx=12

byte Restore_Factory_Setting[9] = {0x7E, 0x00, 0x08, 0x01, 0x00, 0xD9, 0x50, 0xAB, 0xCD};
byte Restore_user_Setting[9] = {0x7E, 0x00, 0x08, 0x01, 0x00, 0xD9, 0x55, 0xAB, 0xCD};

byte Save_user_Setting[9] = {0x7E, 0x00, 0x08, 0x01, 0x00, 0xD9, 0x56, 0xAB, 0xCD};
byte SAVE_TO_FLASH[9] = {0x7E, 0x00, 0x09, 0x01, 0x00, 0x00, 0x00, 0xDE, 0xC8};

byte light_sleep[9] = {0x7E, 0x00, 0x08, 0x01, 0x00, 0xD9, 0xA0, 0xAB, 0xCD};
byte Deep_sleep[9] = {0x7E, 0x00, 0x08, 0x01, 0x00, 0xD9, 0xA0, 0xAB, 0xCD};
byte wake_up[9] = {0x7E, 0x00, 0x08, 0x01, 0x00, 0xD9, 0xA0, 0xAB, 0xCD};

void setup() {
  Serial.begin(115200);
  mySerial.begin(9600);
  
  mySerial.write(Restore_Factory_Setting, 9);
}

void loop() {
 if (mySerial.available()) {
    String receivedData = mySerial.readStringUntil('\n'); // Read entire data until newline character
    
    Serial.println(receivedData); // Print the data to Serial Monitor
  }
}
