#include <SoftwareSerial.h>
SoftwareSerial mySerial(11, 12);  //Rx=11, Tx=12

byte BR_1200[10] = {0x7E, 0x00, 0x08, 0x02, 0x00, 0x2A, 0xC4, 0x09, 0xAB, 0xCD};
byte BR_4800[10] = {0x7E, 0x00, 0x08, 0x02, 0x00, 0x2A, 0x71, 0x02, 0xAB, 0xCD};
byte BR_9600[10] = {0x7E, 0x00, 0x08, 0x02, 0x00, 0x2A, 0x39, 0x01, 0xAB, 0xCD};

byte BR_14400[10] = {0x7E, 0x00, 0x08, 0x02, 0x00, 0x2A, 0xD0, 0x00, 0xAB, 0xCD};
byte BR_19200[10] = {0x7E, 0x00, 0x08, 0x02, 0x00, 0x2A, 0x9C, 0x00, 0xAB, 0xCD};
byte BR_38400[10] = {0x7E, 0x00, 0x08, 0x02, 0x00, 0x2A, 0x4E, 0x00, 0xAB, 0xCD};
byte BR_57600[10] = {0x7E, 0x00, 0x08, 0x02, 0x00, 0x2A, 0x34, 0x00, 0xAB, 0xCD};

byte BR_115200[10] = {0x7E, 0x00, 0x08, 0x02, 0x00, 0x2A, 0x1A, 0x00, 0xAB, 0xCD};

void setup() {
  Serial.begin(115200);
  mySerial.begin(9600);
  
  mySerial.write(BR_9600, 10);
}

void loop() {
 if (mySerial.available()) {
    String receivedData = mySerial.readStringUntil('\n'); // Read entire data until newline character
    
    Serial.println(receivedData); // Print the data to Serial Monitor
  }
}
